{
  "name": "client",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/*.{js,jsx,ts,tsx}",
      "!src/**/*.{cy.js,cy.jsx,cy.ts,cy.tsx}",
      "!src/api-autogenerated/**",
      "!src/app/**",
      "!src/types/**",
      "!src/atoms/**",
      "!src/lang/**"
    ],
    "coverageThreshold": {
      "**": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "cross-env": "^7.0.3",
    "http-proxy-middleware": "^2.0.6",
    "i18next": "^22.5.0",
    "i18next-browser-languagedetector": "^7.0.2",
    "msw": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.11.2",
    "react-scripts": "^5.0.1",
    "recoil": "^0.7.7",
    "sass": "^1.62.1",
    "typescript": "^4.9.5",
    "web-vitals": "^3.3.1"
  }
}
